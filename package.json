{
  "name": "dicedornot",
  "version": "1.0.0",
  "scripts": {
    "build": "snowpack build",
    "start": "snowpack dev",
    "check-types": "svelte-check && tsc --noEmit",
    "validateReplay": "npm run buildJsonSchema && npm run buildJSModules && ts-node ./bin/validateReplay.mjs",
    "buildJsonSchema": "typescript-json-schema --required --noExtraProps --strictNullChecks tsconfig.json ReplayStep > bin/replayStep.json",
    "buildJSModules": "tsc && for file in build/*.js; do mv $file ${file%.js}.mjs; done"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@snowpack/app-scripts-svelte": "^2.0.1",
    "@snowpack/plugin-babel": "^2.1.7",
    "@snowpack/plugin-dotenv": "^2.2.0",
    "@snowpack/plugin-webpack": "^3.0.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/chroma-js": "^2.1.3",
    "@types/he": "^1.1.2",
    "@types/underscore": "^1.11.3",
    "ajv": "^8.6.2",
    "better-ajv-errors": "^0.7.0",
    "snowpack": "^3.8.8",
    "svelte": "^3.42.4",
    "svelte-check": "^2.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "typescript-json-schema": "^0.50.1",
    "typescript-json-validator": "^2.4.2"
  },
  "dependencies": {
    "@babel/preset-env": "^7.15.0",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "bootstrap-icons": "^1.5.0",
    "chroma-js": "^2.1.2",
    "fast-xml-parser": "^3.19.0",
    "he": "^1.2.0",
    "idb-keyval": "^5.1.3",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "sirv-cli": "^1.0.14",
    "sveltestrap": "^5.6.2",
    "underscore": "^1.13.1",
    "vega": "^5.20.2",
    "vega-embed": "^6.18.2",
    "vega-lite": "^5.1.0"
  }
}
