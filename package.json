{
  "name": "dicedornot",
  "version": "1.0.0",
  "scripts": {
    "build": "snowpack build",
    "dev": "snowpack dev",
    "check-types": "svelte-check && tsc --noEmit",
    "validateReplay": "npm run buildJsonSchema && npm run buildJSModules && ts-node ./bin/validateReplay.mjs",
    "buildJsonSchema": "typescript-json-schema --required --noExtraProps --strictNullChecks tsconfig.json ReplayStep > bin/replayStep.json",
    "buildJSModules": "tsc && for file in build/*.js; do mv $file ${file%.js}.mjs; done"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@snowpack/app-scripts-svelte": "^2.0.0",
    "@snowpack/plugin-babel": "^2.1.6",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-webpack": "^2.0.17",
    "@tsconfig/svelte": "^1.0.10",
    "@types/he": "^1.1.1",
    "@types/underscore": "^1.11.2",
    "ajv": "^7.2.4",
    "better-ajv-errors": "^0.7.0",
    "snowpack": "^3.0.13",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "typescript-json-schema": "^0.50.0",
    "typescript-json-validator": "^2.4.2"
  },
  "dependencies": {
    "@babel/preset-env": "^7.13.10",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "bootstrap-icons": "^1.4.0",
    "chroma-js": "^2.1.1",
    "fast-xml-parser": "^3.18.0",
    "he": "^1.2.0",
    "idb-keyval": "^5.0.4",
    "jszip": "^3.6.0",
    "lodash": "^4.17.21",
    "sirv-cli": "^1.0.11",
    "sveltestrap": "^3.13.0",
    "underscore": "^1.12.0",
    "vega": "^5.19.1",
    "vega-embed": "^6.15.2",
    "vega-lite": "^4.17.0"
  }
}
